{
  "name": "MarkdownApplication",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run typings-install && npm run build",
    "start": "npm run server:dev",
    "server:dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 8080 --config webpack.dev.config.js",
    "server:prod": "node ./node_modules/http-server/bin/http-server build --cors",
    "build": "node ./node_modules/webpack/bin/webpack --colors --progress --display-error-details -p --config webpack.prod.config.js",
    "test": "node ./node_modules/karma/bin/karma start",
    "clean": "node ./node_modules/rimraf/bin build target Logs",
    "typings-install": "typings install",
    "typings-install-e": "typings install --ambient --save"
  },
  "dependencies": {
    "angular": "^1.4.9",
    "angular-sanitize": "^1.4.9",
    "appcache-webpack-plugin": "^1.2.1",
    "bootstrap": "^3.3.6",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "exports-loader": "^0.6.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "http-server": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "ngstorage": "^0.3.10",
    "node-sass": "^3.4.2",
    "on-build-webpack": "^0.1.0",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "showdown": "^1.3.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.5.1",
    "ts-loader": "^0.7.2",
    "tslint": "^3.2.1",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.7.5",
    "typings": "^0.6.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  }
}
